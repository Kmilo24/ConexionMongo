// ##endpoints
.get('/endpoints', (req, res) => {
    const eps = require('express-list-endpoints')(module.exports)
    return res.type('html').send(`<ul>
     ${eps.map(ep => `<li title="${ep.methods.slice(0, 4)}"><a href="..${ep.path}/">${ep.path}</a></li>`).join(`\n`)}
      <!-- ${JSON.stringify(eps, null, 2)} -->
      </ul>`)
})